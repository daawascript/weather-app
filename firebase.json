{
  "functions": {
    "predeploy": ["npm --prefix \"$RESOURCE_DIR\" run lint"]
  },
  "hosting": {
    "public": "build",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "rewrites": [
      {
        "source": "/geoCode",
        "function": "geoCode"
      },
      {
        "source": "/weather",
        "function": "weather"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
